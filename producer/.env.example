# RTSP Producer Configuration
# Copy this file to .env and update with your values

# RTSP stream URL
RTSP_URL=rtsp://admin:KAQSML@172.16.6.77:554

# Broker configuration
# For Docker/Caddy setup (HTTPS/HTTP/2): USE_HTTPS=true, BROKER_PORT=3090
# For direct HTTP access: USE_HTTPS=false, BROKER_PORT=3091
USE_HTTPS=true
BROKER_PORT=3090

# SSL verification (set to false for self-signed certificates)
VERIFY_SSL=false

# Stream identifier
STREAM_ID=stream1

# Target frames per second
TARGET_FPS=30

RANDOM_BOXES=true
